{
    "apiVersion": "0.2",
    "swaggerVersion": "1.2",
    "basePath": "http://127.0.0.1:5000",
    "produces":
    [
        "application/json"
    ],
    "apis":
    [
        {
            "path": "/candidate",
            "operations": [
                {
                    "method": "GET",
                    "summary": "Get a list of candidates.",
                    "nickname": "getCandidate",
                    "parameters": [
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"q",
                            "description":"Full-text search"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"/",
                            "description":"Single candidate's record"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"candidate_id",
                            "description":"Synonym for /"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"office",
                            "description":"Governmental office sought"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"state",
                            "description":"Two-letter state abbreviation"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"district",
                            "description":"Two-digit number(00 - if no district or at large)"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"name",
                            "description":"Candidate's name"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"page",
                            "description":"Page number"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"party",
                            "description":"3-letter party abbreviation"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"per_page",
                            "description":"Number of records per page"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"year",
                            "description":"Any year in which candidate ran"
                        },
                        {
                            "paramType": "query",
                            "type": "string",
                            "name":"fields",
                            "description":"Comma separated list of fields to display or * for all",
                            "allowMultiple": true
                        }

                    ]
                }
            ]
        }
    ]
}




